" =========================
"   IMPORT INITIAL SETTING
" =========================
source $HOME/.vimconf/init.vim/general.vim
source $HOME/.vimconf/init.vim/plugin.vim
source $HOME/.vimconf/init.vim/interface.vim
source $HOME/.vimconf/init.vim/filetype.vim
source $HOME/.vimconf/init.vim/keybind.vim

" =========================
"  IMPORT PLUGIN SETTING
" =========================
source $HOME/.vimconf/plugin/airline.vim
source $HOME/.vimconf/plugin/coc.vim
source $HOME/.vimconf/plugin/ctrlp.vim
source $HOME/.vimconf/plugin/dracula.vim
source $HOME/.vimconf/plugin/fzf.vim
source $HOME/.vimconf/plugin/indentLine.vim
source $HOME/.vimconf/plugin/nerdtree.vim
source $HOME/.vimconf/plugin/rust.vim
source $HOME/.vimconf/plugin/syntastic.vim
source $HOME/.vimconf/plugin/tagbar.vim
source $HOME/.vimconf/plugin/vim-devicons.vim
source $HOME/.vimconf/plugin/vim-gitgutter.vim
source $HOME/.vimconf/plugin/vim-markdown.vim

let g:jupytext_command = 'notedown'
let g:jupytext_fmt = 'markdown'
let g:jupytext_to_ipynb_opts = '--to=notebook'
